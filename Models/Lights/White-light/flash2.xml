<?xml version="1.0"?>

<PropertyList>

 <path>beacon-flash.ac</path>

 <animation>
  <type>noshadow</type>
  <object-name>redflash</object-name>
 </animation>

 <animation>
  <type>billboard</type>
  <spherical>true</spherical>
 </animation>

 <animation>
  <type>select</type>
  <object-name>redflash</object-name>
  <condition>
   <greater-than>
      <property>sim/multiplay/generic/float[1]</property>
      <value>0.96</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>dist-scale</type>
  <object-name>redflash</object-name>
  <interpolation>
   <entry><ind>0</ind><dep>1.0</dep></entry>
   <entry><ind>500</ind><dep>2.0</dep></entry>
   <entry><ind>16000</ind><dep>16.0</dep></entry>
  </interpolation>
 </animation>

</PropertyList>
